<Application x:Class="ProjectRunner.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProjectRunner.WPF"
             Startup="AppStartup">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="DefaultBackgroundGray" Color="#e0e0e0" />
            <SolidColorBrush x:Key="HighlightedBackgroundGray" Color="#f5f5f5" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
